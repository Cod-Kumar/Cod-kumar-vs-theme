	"tokenColors": [
		{
		  "name": "Comment",
		  "scope": [
			"comment",
			"punctuation.definition.comment"
		  ],
		  "settings": {
			"fontStyle": "italic",
			"foreground": "#4A5C64"
		  }
		},
		{
		  "name": "Variables",
		  "scope": [
			"variable",
			"string constant.other.placeholder"
		  ],
		  "settings": {
			"foreground": "#B3D1D1"
		  }
		},
		{
		  "name": "Colors",
